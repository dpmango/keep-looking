    include _head

  body(class=bodyClass)
    .page(class=outClass)
      .page__content
        block header
          include ../components/_header

        main.main
          #barba-wrapper
            .barba-container
              block content

      block footer
        include ../components/_footer
      block scripts
        include _scripts
